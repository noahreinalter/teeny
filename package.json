{
    "name": "teeny-site",
    "version": "0.2.6",
    "description": "A very simple static site generator",
    "author": "Yakko Majuri, Noah Reinalter",
    "main": "cli.js",
    "license": "MIT",
    "type": "module",
    "bin": {
        "teeny": "cli.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/noahreinalter/teeny"
    },
    "scripts": {
        "dev:build": "npm run dev && teeny build",
        "dev:develop": "npm run dev && teeny develop",
        "dev:init": "npm run dev && teeny init",
        "dev": "rm teeny-site* || true && npm pack && sudo npm i -g teeny-site*.tgz -f",
        "lint": "prettier --write ."
    },
    "devDependencies": {
        "prettier": "^3.0.0"
    },
    "dependencies": {
        "chokidar": "^3.5.3",
        "front-matter": "^4.0.2",
        "fs-extra": "^11.1.1",
        "jsdom": "^22.1.0",
        "marked": "^7.0.0"
    },
    "files": [
        "cli.js"
    ],
    "keywords": [
        "static-site-generator",
        "static-site",
        "ssg",
        "website",
        "blog",
        "templates",
        "generator",
        "framework",
        "html",
        "markdown",
        "liquid",
        "teeny"
    ]
}
